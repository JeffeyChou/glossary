@article{Abellan2017,
  title = {Analyzing Properties of {{Deng}} Entropy in the Theory of Evidence},
  author = {Abell√°n, Joaqu√≠n},
  date = {2017-02-01},
  journaltitle = {Chaos, Solitons \& Fractals},
  shortjournal = {Chaos, Solitons \& Fractals},
